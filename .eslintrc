{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2022": true,
    "jasmine": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "no-redeclare": "warn",
    "no-self-assign": "warn",
    "no-undef": "warn",
    "no-unused-vars": "warn"
  }
}
